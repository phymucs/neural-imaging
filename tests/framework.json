{
  "train-nip": {
    "log": "train-nip.log",
    "command": "python3 train_nip.py --cam \"{cam}\" --nip INet --split 40:20:1 --epochs 400 --out {root}/nip_snapshots --patch 64",
    "files": [
      "nip_snapshots/{}/inet/progress.json",
      "nip_snapshots/{}/inet/inet-399.meta",
      "nip_snapshots/{}/inet/inet-399.index"
    ],
    "performance" : {
      "file": "nip_snapshots/{}/inet/progress.json",
      "values": {
        "performance/psnr/validation": 40
      }
    }
  },
  "resume-nip": {
    "log": "resume-nip.log",
    "command": "python3 train_nip.py --cam \"{cam}\" --nip INet --split 40:20:1 --epochs 800 --out {root}/nip_snapshots --patch 64 --resume",
    "files": [
      "nip_snapshots/{}/inet/progress.json",
      "nip_snapshots/{}/inet/inet-799.meta",
      "nip_snapshots/{}/inet/inet-799.index"
    ],
    "performance": {
      "file": "nip_snapshots/{}/inet/progress.json",
      "values": {
        "performance/psnr/validation": 41
      }
    }
  },
  "train-manipulation": {
    "log": "train-manipulation.log",
    "command": "python3 train_manipulation.py --cam \"{cam}\" --nip INet --split 40:20:1 --epochs 201 --dir {root}/manipulation --end 1 --nip-dir {root}/nip_snapshots --ln 0 --ds none --patch 64",
    "files": [
      "manipulation/{}/INet/fixed-nip/000/training.json",
      "manipulation/{}/INet/fixed-nip/000/models/fan/fan-200.meta"
    ],
    "performance" : {
      "file": "manipulation/{}/INet/lr-0.1000/000/training.json",
      "values": {
